    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-tsdoc": "^0.2.6",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.0",
